{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*\\.(html|css|js|jpg|jpeg|png|gif|ico|svg))$",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/server.js"
    }
  ]
}